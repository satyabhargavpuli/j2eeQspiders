import org.springframework.core.io.FileSystemResource;

public class App 
{
    @SuppressWarnings("deprecation")
	public static void main( String[] args )
    {
    	BeanFactory factory = new XmlBeanFactory(new FileSystemResource("spring-config.xml"));
    }
}
